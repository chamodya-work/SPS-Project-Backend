	<parameter name="@appNo" class="java.lang.String"/>
	<parameter name="@costctr" class="java.lang.String"/>

	<queryString>
		<![CDATA[
    SELECT b.app_no, e.estimate_no, e.std_cost, b.total_cost, e.partial_amt, e.descr,
           a.REPRESENTATIVE, a.SCHEME_NAME, a.SCHEME_NO, a.FUND_SOURCE, a.DEV_SEC,
           a.ELECTORATE, a.SERVICEDEPONAME, a.area_code, a.aga_division, a.district,
           (SELECT brnach_name
            FROM province_branch
            WHERE dept_id = (SELECT dept_id
                             FROM spestedy_cons
                             WHERE TRIM(westimate_no) = $P{appNo})) AS branch_name,
           (SELECT comp_nm
            FROM glcompm
            WHERE comp_id IN (SELECT comp_id
                              FROM gldeptm
                              WHERE dept_id = a.area_code)) AS area,
           (SELECT comp_nm
            FROM glcompm
            WHERE comp_id IN (SELECT comp_id
                              FROM gldeptm
                              WHERE dept_id = $P{costctr})) AS province,
           (SELECT dept_nm
            FROM gldeptm
            WHERE dept_id = $P{costctr}) AS CCT_NAME
    FROM "SPSNEW"."WIRING_LAND_DETAIL_CON" a,
         "SPSNEW"."APPLICATION_REFERENCE" d,
         "SPSNEW"."ESTIMATE_REFERENCEBS" c,
         "SPSNEW"."SPSTDESTHMT" b,
         "SPSNEW"."PCESTHTT" e
    WHERE TRIM(a.APPLICATION_id) = TRIM(d.APPLICATION_id)
      AND TRIM(d.APPLICATION_NO) = TRIM(b.STD_NO)
      AND TRIM(c.westimate_no) = TRIM(e.estimate_no)
      AND TRIM(d.application_no) = TRIM(c.sestimate_no)
      AND e.dept_id = $P{deptId}
      AND TRIM(e.estimate_no) = $P{applicationNo}
    ]]>
	</queryString>
